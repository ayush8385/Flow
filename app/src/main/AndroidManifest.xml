<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ayush.flow">

    <application
        android:allowBackup="true"
        android:icon="@drawable/flow"
        android:label="@string/app_name"
        android:roundIcon="@drawable/flow"
        android:supportsRtl="true"
        android:theme="@style/Theme.Flow">
        <activity android:name=".flow.Addprofile"
            android:windowSoftInputMode="adjustResize"></activity>
        <activity android:name=".flow.Dashboard" />
        <activity android:name=".flow.Register" />
        <activity android:name=".flow.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>